<?xml version='1.0' encoding='utf-8'?>
<widget id="com.medifax.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SpeechRecognition">
        <param name="android-package" value="com.pbakondy.SpeechRecognition" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Medifax</name>
    <description>
        Medifax mobile app for access to medial summary data.
    </description>
    <author email="bryan@furnaceai.com" href="http://www.furnaceai.com">
        Bryan Richard
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="https://s3.amazonaws.com" />
    <allow-intent href="market:*" />
    <icon height="36" src="res/icon/android/icon-36-ldpi.png" width="36" />
    <icon height="48" src="res/icon/android/icon-48-mdpi.png" width="48" />
    <icon height="72" src="res/icon/android/icon-72-hdpi.png" width="72" />
    <icon height="96" src="res/icon/android/icon-96-xhdpi.png" width="96" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="true" />
</widget>
